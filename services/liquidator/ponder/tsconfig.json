{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "noEmit": true
  },
  "include": ["src/**/*.ts", "ponder.config.ts", "ponder.schema.ts", "abis/**/*.ts"],
  "exclude": ["node_modules"]
}
